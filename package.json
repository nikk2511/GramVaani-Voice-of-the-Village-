{
  "name": "our-voice-our-rights",
  "version": "1.0.0",
  "description": "MGNREGA District Performance Viewer - A mobile-first web app for citizens to view MGNREGA data",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:frontend && npm run test:backend && npm run test:worker",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "test:worker": "cd worker && npm test",
    "test:e2e": "cd frontend && npm run test:e2e",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint"
  },
  "keywords": [
    "mgnrega",
    "government",
    "data",
    "visualization",
    "india"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

